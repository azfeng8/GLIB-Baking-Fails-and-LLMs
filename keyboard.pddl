(:action push-key
:parameters (?finger - finger)
)

(:action reposition-finger
:parameters (?finger - finger ?key - key)
)

(:action reposition-hand
:parameters (?thumb - finger ?key1 - key ?pinky - finger ?key2 - key)
)

(:action push-pedal
:parameters (?depth - depth ?pedal - pedal)
)

(:action lift-pedal
:parameters (?pedal - pedal)
)